<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="GoodClass" table="GoodClass">

          <!-- 主键物品类别id -->
          <id name="goodClassId" column="goodClassId">
            <generator class="native"/>
          </id>

          <!-- 简单字符串属性: 物品类别名称 -->
          <property
            name="goodClassName"
            column="goodClassName"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
