<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="GoodUse" table="GoodUse">

          <!-- 主键领用id -->
          <id name="useId" column="useId">
            <generator class="native"/>
          </id>

          <!-- 多对一属性: 领用物品 -->
          <many-to-one name="goodObj"
            column="goodObj"
            class="Goods"
            lazy="false"
            outer-join="true"
            cascade="save-update"
          />

          <!-- 简单整型属性: 领用数量 -->
          <property
            name="useCount"
            column="useCount"
            type="int"

          />

          <!-- 简单浮点型属性: 单价 -->
          <property
            name="price"
            column="price"
            type="float"
          />

          <!-- 简单浮点型属性: 金额 -->
          <property
            name="totalMoney"
            column="totalMoney"
            type="float"
          />

          <!-- 简单日期型属性: 领用时间 -->
          <property
            name="useTime"
            column="useTime"
            	type="timestamp"
            	length="10"
          />

          <!-- 简单字符串属性: 领用人 -->
          <property
            name="userMan"
            column="userMan"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 经办人 -->
          <property
            name="operatorMan"
            column="operatorMan"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 仓库 -->
          <property
            name="storeHouse"
            column="storeHouse"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
